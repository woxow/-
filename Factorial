import java.util.Scanner;

public class FactoN 
{
	public static void main(String[] args)
	{
		System.out.println("Input number: ");
		Scanner in = new Scanner(System.in);
		int n=in.nextInt();
		int facto=1;
		for(int i=1;i<=n;i++)
		{
			facto*=i;
		}
		System.out.println("Factorial "+n+" = "+facto);
		in.close();
	}
}
